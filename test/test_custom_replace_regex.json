{
    "name":"Sample",
    "env":{
        "logLevel":"debug",
        "replaceRegex":"##([a-zA-Z0-9\\.\\-_]*)##"
    },
    
    "tasks":[
        {
            "type":"set-var",
            "key":"key_of_var",
            "var":{
                "a":"value-a",
                "b":"value-b"
            },
            "varType":"value",
            "fileFormat":"string"
        },
        {
            "type":"output",
            "text":"I found a value key_of_var.a=##key_of_var.a## and key_of_var.b=##key_of_var.b##"
        }
    ]
}