{
    "name":"Sample",
    "env":{
        "logLevel":"debug"
    },
    "tasks":[
        {
            "type":"set-var",
            "key":"src",
            "value":"copy-filter-del-test-src"
        },
        {
            "type":"output",
            "target":"fw",
            "path":"output/${src}/test.txt",
            "text":"copy-test"
        },
        {
            "type":"output",
            "target":"fw",
            "path":"output/${src}/test2.txt",
            "text":"copy-test"
        },
        {
            "type":"output",
            "target":"fw",
            "path":"output/${src}/inner/test2.txt",
            "text":"copy-test"
        },
        {
            "type":"fs-del",
            "path":"output/${src}",
            "include":["./*"],
            "exclude":["test.txt"]
        }
    ]
}