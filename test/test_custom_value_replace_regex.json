{
    "name":"Sample",
    "env":{
        "verbose":true,
        "verboseGit":true,
        "valueReplaceRegex":"##([a-zA-Z0-9\\.\\-_]*)##"
    },
    
    "tasks":[
        {
            "type":"set-value",
            "key":"key_of_value",
            "value":{
                "a":"value-a",
                "b":"value-b"
            },
            "valueType":"value",
            "fileFormat":"string"
        },
        {
            "type":"output",
            "text":"I found a value ##key_of_value.a## and ##key_of_value.b##"
        }
    ]
}